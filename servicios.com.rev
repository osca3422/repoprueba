$ORIGIN 50.168.192.in-addr.arpa.
$TTL 3H
@	IN SOA	server.servicios.com. root@servicios.com. (
					0	; serial
					1D	; refresh
					1H	; retry
					1W	; expire
					3H )	; minimum
@	IN	NS	server.servicios.com.

;host en la zona

2       IN      PTR     cliente.servicios.com.
3	IN	PTR	server.servicios.com.
4	IN	PTR	server.nuevoservicios.com.
